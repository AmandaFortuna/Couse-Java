
public class processamento_casting {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int x;
		double y;
		x = 5;
		y = 2 * x;
		System.out.println(x);
		System.out.println(y);
		
		
		
		int a, b;
		double resultado;
		a = 5;
		b = 2;
		resultado = a / b;
		System.out.println(resultado);
	
	
	}

}
